.emotion-0 {
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  color: #0f0f17;
}

.emotion-0>* {
  padding: 0.5rem;
}

.emotion-1 {
  position: relative;
  width: -webkit-fit-content;
  width: -moz-fit-content;
  width: fit-content;
}

.emotion-2 {
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  color: #1552DD;
  background-color: rgba(84,94,255,0);
  padding: 0.5rem;
  border-radius: 2.25rem;
  border: none;
  cursor: pointer;
  -webkit-transition: background-color,border 150ms linear;
  transition: background-color,border 150ms linear;
}

.emotion-2:focus-visible:not(:disabled) {
  background-color: rgba(84,94,255,0.06);
}

.emotion-2:disabled {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-2:hover:not(:disabled) {
  background-color: rgba(84,94,255,0.06);
}

.emotion-2:active:not(:disabled) {
  background-color: rgba(84,94,255,0.14);
}

.emotion-3 {
  padding: 0.125rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.emotion-4 {
  fill: #1552DD;
  width: 1rem;
  height: 1rem;
}

.emotion-4 path {
  fill: #1552DD;
}

<div>
  <div
    class="emotion-0"
  >
    <div
      class="emotion-1"
    >
      <button
        class="emotion-2"
        data-testid="icon-button"
        disabled=""
        type="button"
      >
        <span
          class="emotion-3"
        >
          <svg
            class="emotion-4"
            viewBox="0 0 24 24"
            xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink"
          >
            <g
              fill-rule="evenodd"
              id="arrow_left"
              stroke="none"
              stroke-width="1"
            >
              <polygon
                fill-rule="nonzero"
                id="Path"
                points="21 6 21 17 19 17 19 7.999 10 8 10 6"
                transform="translate(15.500000, 11.500000) rotate(-135.000000) translate(-15.500000, -11.500000) "
              />
            </g>
          </svg>
        </span>
      </button>
    </div>
    <div>
      Page 
      1
       of 
      3
    </div>
    <div
      class="emotion-1"
    >
      <button
        class="emotion-2"
        data-testid="icon-button"
        type="button"
      >
        <span
          class="emotion-3"
        >
          <svg
            class="emotion-4"
            viewBox="0 0 24 24"
            xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink"
          >
            <g
              fill-rule="evenodd"
              id="arrow_right"
              stroke="none"
              stroke-width="1"
            >
              <polygon
                fill-rule="nonzero"
                id="Path"
                points="15 6 15 17 13 17 13 7.999 4 8 4 6"
                transform="translate(9.500000, 11.500000) scale(-1, 1) rotate(-135.000000) translate(-9.500000, -11.500000) "
              />
            </g>
          </svg>
        </span>
      </button>
    </div>
  </div>
</div>