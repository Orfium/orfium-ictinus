.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

.emotion-1 {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -moz-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  gap: 0.5rem;
}

.emotion-3 {
  font-size: 0.9375rem;
  color: #0f0f17;
}

.emotion-5 {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.emotion-5 .react-switch-handle {
  cursor: unset!important;
  border: 2px solid #b7c3d8!important;
  box-sizing: border-box!important;
  -webkit-transform: translateX(0)!important;
  -moz-transform: translateX(0)!important;
  -ms-transform: translateX(0)!important;
  transform: translateX(0)!important;
}

.emotion-5 .react-switch-handle:hover {
  box-shadow: rgba(14,14,23,0.1) 0 0 0 5px;
  background: #dbe1eb!important;
}

.emotion-5 .react-switch-bg {
  cursor: unset!important;
  margin-right: 0!important;
  margin-left: 0!important;
}

.emotion-5 .react-switch-bg:hover~.react-switch-handle {
  box-shadow: rgba(14,14,23,0.1) 0 0 0 5px;
  background: #dbe1eb!important;
}

<div>
  <div
    class="emotion-0"
  >
    <div
      style="margin: 5px;"
    >
      <label>
        <div
          class="emotion-1 emotion-2"
        >
          <span
            class="emotion-3 emotion-4"
            data-testid="switch_label"
          >
            Label showcase text
          </span>
          <div
            class="emotion-5 emotion-6"
          >
            <div
              style="position: relative; display: inline-block; text-align: left; opacity: 1; direction: ltr; border-radius: 4px; transition: opacity 0.25s; user-select: none;"
            >
              <div
                class="react-switch-bg"
                style="height: 8px; width: 36px; margin: 6px; position: relative; background: rgb(219, 225, 235); border-radius: 4px; cursor: pointer; transition: background 0.25s;"
              />
              <div
                class="react-switch-handle"
                style="height: 20px; width: 20px; background: rgb(243, 245, 248); display: inline-block; cursor: pointer; border-radius: 50%; position: absolute; transform: translateX(0px); top: 0px; outline: 0; border: 0px; transition: background-color 0.25s, transform 0.25s, box-shadow 0.15s;"
              />
              <input
                aria-checked="false"
                data-testid="switch"
                role="switch"
                style="border: 0px; height: 1px; margin: -1px; overflow: hidden; padding: 0px; position: absolute; width: 1px;"
                type="checkbox"
              />
            </div>
          </div>
        </div>
      </label>
    </div>
  </div>
</div>