}

.emotion-4 svg {
  visibility: hidden;
  opacity: 0;
}

.emotion-4:hover,
.emotion-4[data-focus-visible='true'] {
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
  background: rgba(200,206,255,0.20);
