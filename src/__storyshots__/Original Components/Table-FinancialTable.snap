  background: #175BF5;
}

.emotion-4[data-selected='true'] svg,
.emotion-4[data-indeterminate='true'] svg {
  visibility: visible;
  opacity: 1;
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
}

.emotion-4[data-disabled='true'] {
