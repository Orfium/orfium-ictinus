.emotion-0 {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  gap: 1.375rem;
}

.emotion-2 {
  opacity: inherit;
}

.emotion-3 {
  height: 0.1875rem;
  border-radius: 4px;
  background: linear-gradient(to right, #175BF5 0%, #175BF5 0%, #DAE4FB 0%, #DAE4FB 100%);
  cursor: unset!important;
}

.emotion-5 {
  height: 1rem;
  width: 1rem;
  border-radius: 100px;
  background-color: #fff;
  border: 2px solid #175BF5;
  position: absolute;
  z-index: 0;
  cursor: -webkit-grab;
  cursor: grab;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  touch-action: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  box-sizing: border-box;
  cursor: unset!important;
}

.emotion-5::after {
  content: '';
  height: 1rem;
  width: 1rem;
  border: 8px solid transparent;
  border-radius: 100px;
  position: absolute;
  top: -0.625rem;
  left: -0.625rem;
  -webkit-transition: all 0.15s ease-in-out;
  -moz-transition: all 0.15s ease-in-out;
  -ms-transition: all 0.15s ease-in-out;
  -o-transition: all 0.15s ease-in-out;
  -webkit-transition: all 0.15s ease-in-out;
  transition: all 0.15s ease-in-out;
}

.emotion-5:hover::after {
  border-color: rgba(14,14,23,0.1);
}

<div>
  <div
    style="margin-left: 12px; width: 181px;"
  >
    <div
      class="emotion-0 emotion-1"
      data-testid="slider_component"
    >
      <div
        class="emotion-2"
      >
        <div
          class="emotion-3 emotion-4"
          data-testid="track_component"
          style="transform: scale(1); cursor: pointer;"
        >
          <div
            aria-valuemax="100"
            aria-valuemin="0"
            aria-valuenow="0"
            class="emotion-5 emotion-6"
            data-testid="thumb_0"
            draggable="false"
            role="slider"
            style="position: absolute; z-index: 0; cursor: grab; user-select: none; transform: translate(0px, 0px);"
            tabindex="0"
          />
        </div>
      </div>
    </div>
  </div>
</div>