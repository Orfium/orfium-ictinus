.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

.emotion-1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  gap: 16px;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  margin-bottom: 48px;
}

.emotion-2 {
  display: grid;
  grid-template-areas: 'label value' 'bar bar';
  grid-template-columns: 1fr auto;
  gap: 0.25rem;
  width: 100%;
}

.emotion-2:not([aria-valuenow]) .fill {
  width: 50%;
  border-radius: 624.9375rem;
  -webkit-animation: indeterminate 1.7s infinite ease-in-out;
  animation: indeterminate 1.7s infinite ease-in-out;
  will-change: transform;
}

.emotion-3 {
  grid-area: bar;
  background-color: #E9EFFB;
  height: 0.25rem;
  border-radius: 624.9375rem;
  overflow: hidden;
  will-change: transform;
}

.emotion-4 {
  background: #5E8DF8;
  height: 100%;
  border-radius: 624.9375rem;
  width: %;
  -webkit-transition: width 0.5s;
  transition: width 0.5s;
}

.emotion-5 {
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.375rem;
  font-size: 1rem;
  letter-spacing: 0.015625rem;
  color: #111530;
}

.emotion-6 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  gap: 16px;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
}

.emotion-10 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
}

.emotion-12 {
  font-family: Roboto;
  font-weight: 400;
  line-height: 1rem;
  font-size: 0.75rem;
  letter-spacing: 0.015625rem;
  color: #111530;
}

<div>
  <div
    class="emotion-0"
  >
    <div
      style="margin: 5px; width: 25rem;"
    >
      <div
        class="emotion-1"
      >
        <div
          aria-valuemax="100"
          aria-valuemin="0"
          class="emotion-2"
          data-rac=""
          id="react-aria-:rak:"
          role="progressbar"
        >
          <div
            class="bar emotion-3"
            data-testid="undefined_linear_progress_container"
          >
            <div
              class="fill emotion-4"
              data-testid="undefined_linear_progress_value"
            />
          </div>
        </div>
        <p
          class="emotion-5"
        >
          Indeterminate State
        </p>
      </div>
    </div>
    <div
      style="margin: 5px; width: 25rem;"
    >
      <div
        class="emotion-6"
      >
        <div
          aria-valuemax="100"
          aria-valuemin="0"
          class="emotion-2"
          data-rac=""
          id="react-aria-:ran:"
          role="progressbar"
        >
          <div
            class="bar emotion-3"
            data-testid="undefined_linear_progress_container"
          >
            <div
              class="fill emotion-4"
              data-testid="undefined_linear_progress_value"
            />
          </div>
        </div>
        <div
          class="emotion-10"
        >
          <p
            class="emotion-5"
          >
            Determinate State
          </p>
          <p
            class="emotion-12"
          >
            Use the "value" control to create the determinate state
          </p>
        </div>
      </div>
    </div>
  </div>
</div>