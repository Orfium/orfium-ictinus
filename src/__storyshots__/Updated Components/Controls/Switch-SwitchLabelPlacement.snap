  display: block;
  width: 1.25rem;
  height: 1.25rem;
  background: #ffffff;
  border: 0.125rem solid #b9cdfc;
  border-radius: 100%;
  -webkit-transition: all 200ms;
  transition: all 200ms;
}

.emotion-1[data-hovered] .indicator:before,
.emotion-1[data-focus-visible='true'] .indicator:before {
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
  box-shadow: 0px 0px 0px 8px rgba(84,94,255,0.06);
  border-radius: 100%;
}

.emotion-1[data-selected] .indicator:before {
  -webkit-transform: translateX(80%);
  -moz-transform: translateX(80%);
  -ms-transform: translateX(80%);
  transform: translateX(80%);
