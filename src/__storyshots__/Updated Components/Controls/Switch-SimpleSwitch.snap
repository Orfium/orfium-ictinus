.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  gap: 0.75rem;
  position: relative;
  cursor: pointer;
}

.emotion-0 .bar {
  width: 2.25rem;
  height: 0.5rem;
  background: #B8CCFA;
  position: absolute;
  border-radius: 6.25rem;
}

.emotion-0 .indicator {
  width: 2.25rem;
  height: 1.25rem;
  box-sizing: border-box;
  position: relative;
}

.emotion-0 .indicator:before {
  content: '';
  box-sizing: border-box;
  display: block;
  width: 1.25rem;
  height: 1.25rem;
  background: #ffffff;
  border: 0.125rem solid #8EAAEC;
  border-radius: 100%;
  -webkit-transition: all 200ms;
  transition: all 200ms;
}

.emotion-0[data-hovered] .indicator:before,
.emotion-0[data-focus-visible='true'] .indicator:before {
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
  box-shadow: 0px 0px 0px 8px rgba(200,206,255,0.20);
  border-radius: 100%;
}

.emotion-0[data-selected] .indicator:before {
  -webkit-transform: translateX(80%);
  -moz-transform: translateX(80%);
  -ms-transform: translateX(80%);
  transform: translateX(80%);
  background: #175BF5;
  border-color: #175BF5;
}

.emotion-0[data-disabled] {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  gap: 0.125rem;
}

.emotion-2 {
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  color: #111530;
}

<div>
  <label
    class="emotion-0"
    data-rac=""
    data-testid="undefined_undefined_switch"
  >
    <span
      style="border: 0px; clip-path: inset(50%); height: 1px; margin: -1px; overflow: hidden; padding: 0px; position: absolute; width: 1px; white-space: nowrap;"
    >
      <input
        role="switch"
        type="checkbox"
      />
    </span>
    <div
      class="bar"
    />
    <div
      class="indicator"
    />
    <div
      class="emotion-1"
    >
      <div
        class="emotion-2"
        data-testid="undefined_radio_undefined_label"
      >
        Option
      </div>
    </div>
  </label>
</div>