.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

.emotion-1 {
  display: grid;
  grid-template-areas: 'label value' 'bar bar';
  grid-template-columns: 1fr auto;
  gap: 0.25rem;
  width: 100%;
}

.emotion-1:not([aria-valuenow]) .fill {
  width: 50%;
  border-radius: 6.25rem;
  -webkit-animation: indeterminate 1.7s infinite ease-in-out;
  animation: indeterminate 1.7s infinite ease-in-out;
  will-change: transform;
}

.emotion-2 {
  grid-area: bar;
  background-color: #E9EFFB;
  height: 0.25rem;
  border-radius: 6.25rem;
  overflow: hidden;
  will-change: transform;
}

.emotion-3 {
  background: #5E8DF8;
  height: 100%;
  border-radius: 6.25rem;
  width: %;
  -webkit-transition: width 0.5s;
  transition: width 0.5s;
}

<div>
  <div
    class="emotion-0"
  >
    <div
      style="margin: 5px; width: 18.75rem;"
    >
      <div
        aria-valuemax="100"
        aria-valuemin="0"
        class="emotion-1"
        data-rac=""
        id="react-aria-:ra2:"
        role="progressbar"
      >
        <div
          class="bar emotion-2"
          data-testid="undefined_linear_progress_container"
        >
          <div
            class="fill emotion-3"
            data-testid="undefined_linear_progress_value"
          />
        </div>
      </div>
    </div>
  </div>
</div>