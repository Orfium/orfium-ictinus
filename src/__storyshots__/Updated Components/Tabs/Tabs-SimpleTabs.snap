}

.emotion-1[data-orientation='vertical'] [role='tab'] {
  padding: 0.75rem 0.5rem;
  box-shadow: inset 0.125rem 0 0 0 rgba(200,206,255,0);
  -webkit-transition: color ease-in-out 0.2s;
  transition: color ease-in-out 0.2s;
