.emotion-0 {
  display: inline-block;
  border: 1px solid rgba(200,206,255,0.70);
  border-radius: 4px;
  background: #ffffff;
}

.emotion-1 {
  width: 100%;
  border-collapse: collapse;
  table-layout: fixed;
}

.emotion-1 thead>tr>th:last-child,
.emotion-1 tbody>tr>td:last-child {
  border-right: none;
}

.emotion-1 tbody>tr:last-child>td {
  border-bottom: none;
}

.emotion-2 {
  box-shadow: 0 1px 0 0 rgba(200,206,255,0.70);
  display: block;
  width: calc(100%);
  padding-right: 0px;
  box-sizing: border-box;
}

.emotion-2 tr {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.emotion-4 {
  width: 100%;
  height: 2.75rem;
  -webkit-align-content: center;
  -ms-flex-line-pack: center;
  align-content: center;
  text-align: left;
  box-sizing: border-box;
  padding: 8px 16px;
  color: #52567a;
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
}

.emotion-4 [data-header-role='options'] {
  opacity: 0;
}

.emotion-4:hover [data-header-role='options'],
.emotion-4:focus-visible [data-header-role='options'] {
  opacity: 1;
}

.emotion-5 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  gap: 8px;
}

.emotion-6 {
  text-wrap: nowrap;
}

.emotion-16 {
  display: block;
  overflow-y: auto;
  max-height: 280px;
}

.emotion-16 tr {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

.emotion-18 {
