  padding: 0.5rem 1rem;
  color: #52567a;
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
}

.emotion-4 [data-header-role='options'] button {
  opacity: 0;
}

.emotion-4:hover [data-header-role='options'] button,
.emotion-4:focus-visible [data-header-role='options'] button {
