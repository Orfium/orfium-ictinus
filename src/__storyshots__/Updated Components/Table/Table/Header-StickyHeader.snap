  border-radius: 0.25rem;
  background: #ffffff;
}

.emotion-1 {
  width: 100%;
  border-collapse: collapse;
  table-layout: fixed;
}

.emotion-1 thead>tr>th:last-child,
.emotion-1 tbody>tr>td:last-child {
  border-right: none;
}

.emotion-1 tbody>tr:last-child>td {
