.emotion-0 {
  display: inline-block;
  border: 1px solid rgba(200,206,255,0.70);
  border-radius: 4px;
  background: #ffffff;
}

.emotion-1 {
  width: 100%;
  border-collapse: collapse;
  table-layout: fixed;
}

.emotion-1 thead>tr>th:last-child,
.emotion-1 tbody>tr>td:last-child {
  border-right: none;
}

.emotion-1 tbody>tr:last-child>td {
  border-bottom: none;
}

.emotion-2 {
  box-shadow: 0px 1px 1px rgba(200,206,255,0.70);
}

.emotion-3 {
  width: 150%;
  height: 2.75rem;
  -webkit-align-content: center;
  -ms-flex-line-pack: center;
  align-content: center;
  text-align: left;
  box-sizing: border-box;
  padding: 8px 16px;
  color: #52567a;
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
}

.emotion-3 [data-header-role='options'] {
  opacity: 0;
}

.emotion-3:hover [data-header-role='options'],
.emotion-3:focus-visible [data-header-role='options'] {
  opacity: 1;
}

.emotion-4 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  gap: 8px;
}

.emotion-5 {
  text-wrap: nowrap;
}

.emotion-15 {
  height: 2.75rem;
  padding: 8px 16px;
  border-bottom: 1px solid rgba(200,206,255,0.70);
  box-sizing: border-box;
  -webkit-align-content: center;
  -ms-flex-line-pack: center;
  align-content: center;
}

.emotion-16 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

