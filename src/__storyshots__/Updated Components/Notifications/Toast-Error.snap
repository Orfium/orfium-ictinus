.emotion-0 {
  position: relative;
  width: -webkit-fit-content;
  width: -moz-fit-content;
  width: fit-content;
}

.emotion-1 {
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  color: #ffffff;
  height: 2.25rem;
  background-color: #175BF5;
  padding: 0rem 1rem 0rem 1rem;
  border-radius: 0.25rem;
  border: none;
  cursor: pointer;
  -webkit-transition: background-color,border 150ms linear;
  transition: background-color,border 150ms linear;
}

.emotion-1:focus-visible:not(:disabled) {
  background-color: #5E8DF8;
}

.emotion-1:disabled {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-1:hover:not(:disabled) {
  background-color: #5E8DF8;
}

.emotion-1:active:not(:disabled),
.emotion-1[aria-expanded="true"] {
  background-color: #194DCC;
}

.emotion-2 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  gap: 0.5rem;
}

<div>
  <div
    class="emotion-0"
  >
    <button
      class="emotion-1"
      data-slot="button"
      data-testid="button"
      type="button"
    >
      <span
        class="emotion-2"
      >
        <span>
          Error
        </span>
      </span>
    </button>
  </div>
</div>