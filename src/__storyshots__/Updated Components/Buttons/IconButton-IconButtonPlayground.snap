.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-box-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

.emotion-1 {
  position: relative;
  width: -webkit-fit-content;
  width: -moz-fit-content;
  width: fit-content;
}

.emotion-2 {
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  color: #ffffff;
  width: 2.25rem;
  height: 2.25rem;
  background-color: #175bf5;
  padding: 0;
  border-radius: 2.25rem;
  border: none;
  cursor: pointer;
  -webkit-transition: background-color,border 150ms linear;
  transition: background-color,border 150ms linear;
}

.emotion-2:focus-visible:not(:disabled) {
  background-color: #1552DD;
}

.emotion-2:disabled {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-2:hover:not(:disabled) {
  background-color: #1552DD;
}

.emotion-2:active:not(:disabled) {
  background-color: #1249C4;
}

.emotion-3 {
  padding: 0.125rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.emotion-4 {
  fill: #ffffff;
  width: 1rem;
  height: 1rem;
}

.emotion-4 path {
  fill: #ffffff;
}

<div>
  <div
    class="emotion-0"
  >
    <div
      style="margin: 5px;"
    >
      <div
        class="emotion-1"
      >
        <button
          class="emotion-2"
          data-testid="icon-button"
          type="button"
        >
          <span
            class="emotion-3"
          >
            <svg
              class="emotion-4"
              height="12px"
              viewBox="0 0 12 6"
              width="12px"
              xmlns="http://www.w3.org/2000/svg"
              xmlns:xlink="http://www.w3.org/1999/xlink"
            >
              <g
                fill-rule="evenodd"
                id="Page-1"
                stroke="none"
                stroke-width="1"
              >
                <polygon
                  fill-rule="nonzero"
                  id="thin_check"
                  points="1.35355339 3.64644661 0.646446609 4.35355339 5 8.70710678 12.8702016 0.836905201 12.1630948 0.12979842 5 7.29289322"
                />
              </g>
            </svg>
          </span>
        </button>
      </div>
    </div>
  </div>
</div>