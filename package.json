{
  "name": "@orfium/ictinus",
  "version": "5.39.3",
  "type": "module",
  "license": "MIT",
  "overrides": {
    "trim": "^1.0.0",
    "glob-parent": "^6.0.1",
    "postcss": "^8.4.31",
    "trim-newlines": "^3.0.1",
    "string-width": "^4"
  },
  "keywords": [
    "Orfium",
    "typescript",
    "react",
    "styled-system",
    "emotion",
    "design system"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "turbo dev --env-mode=loose",
    "build": "turbo build --env-mode=loose",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:coverage": "turbo run test:coverage",
    "coverage:report": "turbo build --filter='./packages/*' && vitest run --coverage",
    "documentation:generate": "turbo build --filter='./packages/*' && typedoc",
    "documentation:up": "node server/docs-server.js",
    "check": "turbo check",
    "check:ts": "turbo check:ts --affected",
    "check:linting": "turbo run check:linting --affected",
    "check:precommit": "turbo run check:precommit --affected",
    "check:formatting": "turbo run check:formatting",
    "clean": "rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "release": "turbo run build --filter='./packages/*' && changeset publish",
    "release:experimental": "turbo run build --filter='./packages/*' && changeset publish --tag experimental"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orfium/orfium-ictinus.git"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "husky": "^9.1.7",
    "turbo": "^2.5.6",
    "typedoc": "^0.28.11",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.5.0",
    "vite-plugin-turbosnap": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/jest": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/serialize": "^1.3.3",
    "@emotion/styled": "^11.11.0",
    "chromatic": "^13.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "typescript": "^5.9.2"
  },
  "bugs": {
    "url": "https://github.com/Orfium/orfium-ictinus/issues"
  },
  "homepage": "https://github.com/Orfium/orfium-ictinus#readme",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  }
}
