    "postinstall": "patch-package",
    "prepublishOnly": "pnpm build:lib",
    "prepare": "husky install",
    "start": "storybook dev -p 6006",
    "prebuild": "rimraf dist",
    "build:main": "vite build",
    "build:codemods": "vite build --config vite.codemods.config.ts",
    "build:vars": "vite build --config vite.vars.config.ts",
    "build:lib": "pnpm prebuild && pnpm build:main && pnpm build:vars && pnpm build:codemods",
    "build": "storybook build -o build/",
    "test": "vitest --run",
    "test:storyshots": "pnpm test storybook.test.tsx --run",
    "test:watch": "vitest --watch",
    "test:update": "vitest -u",
    "documentation:generate": "typedoc --out typeDocs src",
    "documentation:up": "node server/docs-server.js",
    "chromatic": "chromatic",
    "build-storybook": "pnpm build",
    "yalc:push": "yalc publish --push",
    "lint-staged": "lint-staged --verbose",
    "check": "pnpm check:ts && pnpm check:linting && pnpm check:formatting",
    "check:ts": "tsc --noEmit --pretty",
    "check:linting": "eslint src --color",
    "check:formatting": "prettier src --check",
    "release": "pnpm build:lib && changeset publish"
