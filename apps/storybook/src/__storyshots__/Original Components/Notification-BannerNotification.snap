.emotion-0 {
  position: relative;
  width: -webkit-fit-content;
  width: -moz-fit-content;
  width: fit-content;
}

.emotion-1 {
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-flex-shrink: 0;
  -ms-flex-negative: 0;
  flex-shrink: 0;
  gap: var(--ictinus-spacing-4);
  color: var(--ictinus-color-text-inverted-primary);
  height: var(--ictinus-sizing-9);
  background-color: var(--ictinus-color-palette-primary-base);
  padding: var(--ictinus-spacing-0) var(--ictinus-spacing-6) var(--ictinus-spacing-0) var(--ictinus-spacing-6);
  border-radius: var(--ictinus-border-radius-2);
  border: none;
  cursor: pointer;
  -webkit-transition: background-color,border 150ms linear;
  transition: background-color,border 150ms linear;
}

.emotion-1:focus-visible:not(:disabled) {
  background-color: var(--ictinus-color-palette-primary-muted);
}

.emotion-1:disabled {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-1:hover:not(:disabled) {
  background-color: var(--ictinus-color-palette-primary-muted);
}

.emotion-1:active:not(:disabled),
.emotion-1[aria-expanded="true"] {
  background-color: var(--ictinus-color-palette-primary-contrast);
}

.emotion-2 {
  font-family: Roboto;
  font-weight: 500;
  line-height: 1rem;
  font-size: 0.75rem;
  letter-spacing: 0.015625rem;
  color: var(--ictinus-color-text-default-secondary);
}

<div>
