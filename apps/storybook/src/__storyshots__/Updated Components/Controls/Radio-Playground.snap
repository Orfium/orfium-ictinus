}

.emotion-2 label:hover::before {
  background: rgba(200,206,255,0.20);
  box-shadow: 0px 0px 0px 8px rgba(200,206,255,0.20);
}

.emotion-2 label[data-selected='true']:before {
  border-color: #175BF5;
  border-radius: 50%;
}

.emotion-2 label[data-selected='true']:hover::before {
  background: none;
  box-shadow: none;
}

.emotion-2 label[data-selected='true']:after {
  content: '';
  display: block;
  position: absolute;
  border-radius: 50%;
  width: 0.75rem;
  height: 0.75rem;
  background: #175BF5;
  left: 4px;
  -webkit-animation: circle 0.15s ease-in-out;
  animation: circle 0.15s ease-in-out;
}

.emotion-2 [data-disabled='true'] {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-2 [data-disabled='true']:hover::before {
  background: none;
  box-shadow: none;
}

