.emotion-0 [data-disabled] {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-1 {
  cursor: pointer;
}

.emotion-1>div>div:first-of-type {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  gap: var(--ictinus-spacing-5);
  position: relative;
}

.emotion-1 .bar {
  width: var(--ictinus-sizing-9);
  height: var(--ictinus-sizing-2);
  background: var(--ictinus-color-palette-primary-alt-contrast);
  position: absolute;
  border-radius: var(--ictinus-border-radius-7);
}

.emotion-1 .indicator {
  width: var(--ictinus-sizing-9);
  height: var(--ictinus-sizing-5);
  box-sizing: border-box;
  position: relative;
}

.emotion-1 .indicator:before {
  content: '';
  box-sizing: border-box;
  display: block;
  width: var(--ictinus-sizing-5);
  height: var(--ictinus-sizing-5);
  background: var(--ictinus-color-background-default);
  border: var(--ictinus-border-width-2) solid var(--ictinus-color-border-color-interactive-default);
  border-radius: 100%;
  -webkit-transition: all 200ms;
  transition: all 200ms;
}

.emotion-1[data-hovered] .indicator:before,
.emotion-1[data-focus-visible='true'] .indicator:before {
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
  box-shadow: 0px 0px 0px 8px rgba(200,206,255,0.20);
  border-radius: 100%;
}

.emotion-1[data-selected] .indicator:before {
  -webkit-transform: translateX(80%);
  -moz-transform: translateX(80%);
  -ms-transform: translateX(80%);
  transform: translateX(80%);
  background: var(--ictinus-color-palette-primary-base);
  border-color: var(--ictinus-color-border-color-interactive-active);
}

.emotion-2 {
  -webkit-flex-direction: row;
  -ms-flex-direction: row;
  flex-direction: row;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  gap: 0.75rem;
  -webkit-box-pack: unset;
  -ms-flex-pack: unset;
  -webkit-justify-content: unset;
  justify-content: unset;
}

.emotion-6 {
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  color: var(--ictinus-color-text-default-primary);
}

<div>
  <div
    class="emotion-0"
  >
    <label
      class="emotion-1"
      data-rac=""
      data-react-aria-pressable="true"
      data-testid="ictinus_switch"
    >
      <span
        style="border: 0px; clip-path: inset(50%); height: 1px; margin: -1px; overflow: hidden; padding: 0px; position: absolute; width: 1px; white-space: nowrap;"
      >
        <input
          data-react-aria-pressable="true"
          role="switch"
          tabindex="0"
          type="checkbox"
        />
      </span>
      <div
        class="emotion-2 emotion-3"
      >
        <div
          class="emotion-4 emotion-3"
        >
          <div
            class="bar"
          />
          <div
            class="indicator"
          />
        </div>
        <div
          class="emotion-6"
          data-testid="ictinus_switch_undefined_label"
        >
          Option
        </div>
      </div>
    </label>
  </div>
</div>