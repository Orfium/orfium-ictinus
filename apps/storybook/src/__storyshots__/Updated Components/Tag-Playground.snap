.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  height: 1.5rem;
  width: -webkit-fit-content;
  width: -moz-fit-content;
  width: fit-content;
  box-sizing: border-box;
  gap: var(--ictinus-spacing-3);
  padding: var(--ictinus-spacing-2) var(--ictinus-spacing-4);
  cursor: auto;
  background: #ffffff;
  color: #194DCC;
  border: var(--ictinus-border-width-1) solid;
  border-color: rgba(200,206,255,0.70);
  border-radius: var(--ictinus-border-radius-2);
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.emotion-1 {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

<div>
  <div>
    <div
      aria-label="Label"
      aria-selected="false"
      class="emotion-0"
      data-testid="undefined_tag_container"
      tabindex="-1"
    >
      <div
        class="emotion-1"
      >
        Label
      </div>
    </div>
  </div>
</div>