.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  -webkit-box-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

.emotion-1 {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: -ms-inline-flexbox;
  display: inline-flex;
  gap: var(--ictinus-spacing-3);
  color: var(--ictinus-color-text-default-active);
  -webkit-text-decoration: none;
  text-decoration: none;
  position: relative;
  opacity: 1;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  cursor: pointer;
  pointer-events: auto;
  -webkit-text-decoration: underline;
  text-decoration: underline;
  font-weight: var(--ictinus-weight-medium);
  font-size: var(--ictinus-font-size-4);
  line-height: var(--ictinus-line-height-5);
  letter-spacing: var(--ictinus-letter-spacing-1);
}

.emotion-1:hover,
.emotion-1[aria-expanded='true'] {
  color: var(--ictinus-color-text-default-primary);
}

.emotion-1:hover path,
.emotion-1[aria-expanded='true'] path {
  fill: var(--ictinus-color-text-default-primary)!important;
}

.emotion-1:visited {
  color: var(--ictinus-color-text-default-visited);
}

.emotion-1:visited path {
  fill: var(--ictinus-color-text-default-visited)!important;
}

.emotion-1:focus-visible:after {
  content: '';
  position: absolute;
  inset: -3px -6px;
  border-radius: var(--ictinus-border-radius-2);
  border: var(--ictinus-border-width-3) solid var(--ictinus-color-border-color-interactive-upsell);
}

.emotion-2 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  gap: var(--ictinus-spacing-3);
  color: var(--ictinus-color-text-default-active);
  -webkit-text-decoration: none;
  text-decoration: none;
  position: relative;
  opacity: 1;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  cursor: pointer;
  pointer-events: auto;
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.375rem;
  font-size: 1rem;
  letter-spacing: 0.00625rem;
}

.emotion-2:hover,
.emotion-2[aria-expanded='true'] {
  color: var(--ictinus-color-text-default-primary);
}

.emotion-2:hover path,
.emotion-2[aria-expanded='true'] path {
  fill: var(--ictinus-color-text-default-primary)!important;
}

.emotion-2:visited {
  color: var(--ictinus-color-text-default-visited);
}

.emotion-2:visited path {
  fill: var(--ictinus-color-text-default-visited)!important;
}

.emotion-2:focus-visible:after {
  content: '';
  position: absolute;
  inset: -3px -6px;
  border-radius: var(--ictinus-border-radius-2);
  border: var(--ictinus-border-width-3) solid var(--ictinus-color-border-color-interactive-upsell);
}

<div>
  <div
    class="emotion-0"
  >
    <div
      style="margin: 5px;"
    >
      <div>
        This is an
         
        <a
          class="emotion-1"
          data-slot="link"
          data-testid="_link"
          href="#"
        >
          inline Link
        </a>
        .
      </div>
    </div>
    <div
      style="margin: 5px;"
    >
      <div>
        This is a
        <a
          class="emotion-2"
          data-slot="link"
          data-testid="_link"
          href="#"
        >
          block Link
        </a>
        .
      </div>
    </div>
  </div>
</div>