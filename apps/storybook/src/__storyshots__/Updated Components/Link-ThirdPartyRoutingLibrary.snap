.emotion-0 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  gap: var(--ictinus-spacing-3);
  color: var(--ictinus-color-text-default-active);
  -webkit-text-decoration: none;
  text-decoration: none;
  position: relative;
  opacity: 1;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  cursor: pointer;
  pointer-events: auto;
  font-family: Roboto;
  font-weight: 500;
  line-height: 1.375rem;
  font-size: 1rem;
  letter-spacing: 0.00625rem;
}

.emotion-0:hover,
.emotion-0[aria-expanded='true'] {
  color: var(--ictinus-color-text-default-primary);
}

.emotion-0:hover path,
.emotion-0[aria-expanded='true'] path {
  fill: var(--ictinus-color-text-default-primary)!important;
}

.emotion-0:visited {
  color: var(--ictinus-color-text-default-visited);
}

.emotion-0:visited path {
  fill: var(--ictinus-color-text-default-visited)!important;
}

.emotion-0:focus-visible:after {
  content: '';
  position: absolute;
  inset: -3px -6px;
  border-radius: var(--ictinus-border-radius-2);
  border: var(--ictinus-border-width-3) solid var(--ictinus-color-border-color-interactive-upsell);
}

<div>
  <a
    class="emotion-0"
    data-slot="link"
    data-testid="_link"
    href="/"
  >
    Link
  </a>
</div>