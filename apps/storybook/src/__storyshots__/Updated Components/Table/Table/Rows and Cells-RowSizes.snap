  height: var(--ictinus-sizing-11);
  -webkit-align-content: center;
  -ms-flex-line-pack: center;
  align-content: center;
  box-sizing: border-box;
  padding: var(--ictinus-spacing-4) var(--ictinus-spacing-6);
  color: var(--ictinus-color-text-default-secondary);
  font-family: Roboto;
  font-weight: 400;
  line-height: 1.25rem;
  font-size: 0.875rem;
  letter-spacing: 0.015625rem;
}

.emotion-4 [data-header-role='options'],
.emotion-4 [data-header-role='sorting-button'] {
  opacity: 0;
}

.emotion-4:hover [data-header-role='options'],
.emotion-4:focus-visible [data-header-role='options'],
.emotion-4:hover [data-header-role='sorting-button'],
.emotion-4:focus-visible [data-header-role='sorting-button'] {
  opacity: 1;
}

.emotion-4 button:focus-visible {
  opacity: 1;
}

.emotion-5 {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: start;
  -ms-flex-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  gap: var(--ictinus-spacing-4);
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.emotion-6 {
  text-wrap: nowrap;
}

.emotion-18 {
  width: 25%;
  height: var(--ictinus-sizing-11);
  padding: var(--ictinus-spacing-4) var(--ictinus-spacing-6);
