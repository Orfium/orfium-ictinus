  padding: var(--ictinus-spacing-0) var(--ictinus-spacing-6) var(--ictinus-spacing-0) var(--ictinus-spacing-6);
  border-radius: var(--ictinus-border-radius-2);
  border: none;
  cursor: pointer;
  -webkit-transition: background-color,border 150ms linear;
  transition: background-color,border 150ms linear;
}

.emotion-3:focus-visible:not(:disabled) {
  background-color: var(--ictinus-color-palette-primary-muted);
}

.emotion-3:disabled {
  opacity: 50%;
  cursor: not-allowed;
}

.emotion-3:hover:not(:disabled) {
  background-color: var(--ictinus-color-palette-primary-muted);
}

.emotion-3:active:not(:disabled),
.emotion-3[aria-expanded="true"] {
  background-color: var(--ictinus-color-palette-primary-contrast);
}

