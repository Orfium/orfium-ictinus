{
  "presets": [
    [
      "@babel/preset-env",
      {
        "shippedProposals": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript",
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".ts", ".tsx", ".js", ".jsx"],
        "root": ["./src"]
      }
    ]
  ]
}
